{
  "name": "@navikt/ft-sak-meny",
  "description": "Meny som viser diverse handlinger en kan utføre på en fagsak",
  "version": "1.0.5",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --colors",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\"",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\"",
    "less:lint": "stylelint \"src/**/*.less\"",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly --downlevelIteration --resolveJsonModule --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules",
    "storybook": "cross-env start-storybook -p 7021"
  },
  "dependencies": {
    "@navikt/ft-form-hooks": "^1.0.40",
    "@navikt/ft-form-validators": "^1.0.28",
    "@navikt/ft-plattform-komponenter": "^1.0.33",
    "@navikt/ft-ui-komponenter": "^1.0.37",
    "@navikt/ft-utils": "^1.0.33",
    "nav-frontend-core": "6.0.1",
    "nav-frontend-grid": "2.0.2",
    "nav-frontend-grid-style": "1.0.3",
    "nav-frontend-knapper": "3.1.3",
    "nav-frontend-knapper-style": "2.1.2",
    "nav-frontend-modal": "2.0.2",
    "nav-frontend-modal-style": "2.0.2",
    "nav-frontend-typografi": "4.0.2",
    "nav-frontend-typografi-style": "2.0.2",
    "react": "18.2.0",
    "react-hook-form": "7.34.2",
    "react-intl": "6.1.0"
  },
  "peerDependencies": {
    "@navikt/ft-form-hooks": "1.x",
    "@navikt/ft-form-validators": "1.x",
    "@navikt/ft-plattform-komponenter": "1.x",
    "@navikt/ft-ui-komponenter": "1.x",
    "@navikt/ft-utils": "1.x",
    "nav-frontend-core": "6.x",
    "nav-frontend-grid": "2.x",
    "nav-frontend-grid-style": "1.x",
    "nav-frontend-knapper": "3.x",
    "nav-frontend-knapper-style": "2.x",
    "nav-frontend-modal": "2.x",
    "nav-frontend-modal-style": "2.x",
    "nav-frontend-typografi": "4.x",
    "nav-frontend-typografi-style": "2.x",
    "react": "18.x",
    "react-hook-form": "7.x",
    "react-intl": "6.1.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.5.10",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.4.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": "./dist/index.js",
    "./dist/style.css": "./dist/style.css"
  }
}

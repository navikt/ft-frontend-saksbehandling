{
  "name": "@navikt/ft-sak-meny",
  "description": "Meny som viser diverse handlinger en kan utføre på en fagsak",
  "version": "3.3.17",
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7021"
  },
  "dependencies": {
    "@navikt/ds-css": "6.12.0",
    "@navikt/ds-react": "6.12.0",
    "@navikt/ft-form-hooks": "^4.4.14",
    "@navikt/ft-form-validators": "^2.3.13",
    "@navikt/ft-plattform-komponenter": "^2.5.17",
    "@navikt/ft-ui-komponenter": "^2.3.14",
    "@navikt/ft-utils": "^2.3.13",
    "react": "18.3.1",
    "react-hook-form": "7.52.0",
    "react-intl": "6.6.8"
  },
  "peerDependencies": {
    "@navikt/ds-css": "2.8.11",
    "@navikt/ds-react": "2.8.11",
    "@navikt/ft-form-hooks": "4.x",
    "@navikt/ft-form-validators": "2.x",
    "@navikt/ft-plattform-komponenter": "2.x",
    "@navikt/ft-ui-komponenter": "2.x",
    "@navikt/ft-utils": "2.x",
    "react": "18.2.0",
    "react-hook-form": "7.43.9",
    "react-intl": "6.3.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "8.1.10",
    "@storybook/react": "8.1.10",
    "@storybook/react-vite": "8.1.10",
    "@testing-library/react": "14.3.1",
    "@testing-library/user-event": "14.5.2",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "storybook": "8.1.10",
    "stylelint": "16.6.1",
    "typescript": "5.5.2",
    "vite": "5.3.1",
    "vitest": "1.6.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

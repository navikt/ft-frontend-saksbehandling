{
  "name": "@navikt/ft-sak-meny",
  "description": "Meny som viser diverse handlinger en kan utføre på en fagsak",
  "version": "1.3.15",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --colors",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\"",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\"",
    "less:lint": "stylelint \"src/**/*.less\"",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly --downlevelIteration --resolveJsonModule --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules",
    "storybook": "cross-env start-storybook -p 7021"
  },
  "dependencies": {
    "@navikt/ds-css": "2.1.5",
    "@navikt/ds-react": "2.1.5",
    "@navikt/ft-form-hooks": "^3.3.0",
    "@navikt/ft-form-validators": "^1.0.48",
    "@navikt/ft-plattform-komponenter": "^1.4.9",
    "@navikt/ft-ui-komponenter": "^1.3.11",
    "@navikt/ft-utils": "^1.1.16",
    "react": "18.2.0",
    "react-hook-form": "7.43.0",
    "react-intl": "6.2.7"
  },
  "peerDependencies": {
    "@navikt/ds-css": "2.1.5",
    "@navikt/ds-react": "2.1.5",
    "@navikt/ft-form-hooks": "3.x",
    "@navikt/ft-form-validators": "1.x",
    "@navikt/ft-plattform-komponenter": "1.x",
    "@navikt/ft-ui-komponenter": "1.x",
    "@navikt/ft-utils": "1.x",
    "react": "18.2.0",
    "react-hook-form": "7.43.0",
    "react-intl": "6.2.7"
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.5.16",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": "./dist/index.js",
    "./dist/style.css": "./dist/style.css"
  }
}

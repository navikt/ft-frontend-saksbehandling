{
  "name": "@navikt/ft-form-validators",
  "description": "Valideringsregler for bruk i forms",
  "version": "2.0.27",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules"
  },
  "dependencies": {
    "@navikt/ft-utils": "^2.0.26",
    "moment": "2.29.4"
  },
  "peerDependencies": {
    "@navikt/ft-utils": "2.x",
    "moment": "2.29.4"
  },
  "devDependencies": {
    "@storybook/react": "7.0.18",
    "cross-env": "7.0.3",
    "eslint": "8.41.0",
    "typescript": "5.0.4",
    "vite": "4.3.9",
    "vitest": "0.31.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  }
}

{
  "name": "@navikt/ft-tidslinje",
  "description": "Overbygg for vis-timeline",
  "version": "1.0.29",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --passWithNoTests --coverage --colors",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\"",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\"",
    "less:lint": "stylelint \"src/**/*.less\"",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly --downlevelIteration --resolveJsonModule --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules"
  },
  "dependencies": {
    "@navikt/ft-kodeverk": "^1.0.16",
    "@navikt/ft-types": "^1.0.26",
    "@navikt/ft-ui-komponenter": "^1.0.35",
    "@navikt/ft-utils": "^1.0.32",
    "moment": "2.29.4",
    "nav-frontend-core": "6.0.1",
    "nav-frontend-grid": "2.0.2",
    "nav-frontend-grid-style": "1.0.3",
    "nav-frontend-ikoner-assets": "3.0.1",
    "nav-frontend-js-utils": "1.0.20",
    "react": "17.0.2",
    "react-intl": "6.0.5",
    "vis-data": "7.1.4",
    "vis-timeline": "7.7.0"
  },
  "peerDependencies": {
    "@navikt/ft-kodeverk": "^1.x",
    "@navikt/ft-types": "^1.x",
    "@navikt/ft-ui-komponenter": "^1.x",
    "@navikt/ft-utils": "^1.x",
    "moment": "2.x",
    "nav-frontend-core": "6.x",
    "nav-frontend-grid": "2.x",
    "nav-frontend-grid-style": "1.x",
    "nav-frontend-ikoner-assets": "3.x",
    "nav-frontend-js-utils": "1.x",
    "react": "17.x",
    "react-intl": "6.x",
    "vis-data": "7.x",
    "vis-timeline": "7.x"
  },
  "devDependencies": {
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "typescript": "4.8.2",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "3.0.0"
  },
  "resolutions": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": "./dist/index.js",
    "./dist/style.css": "./dist/style.css"
  }
}

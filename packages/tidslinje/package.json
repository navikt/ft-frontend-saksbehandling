{
  "name": "@navikt/ft-tidslinje",
  "description": "Overbygg for vis-timeline",
  "version": "2.0.10",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules",
    "build-storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider build-storybook -o .storybook-static-build",
    "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook --quiet -p 7022"
  },
  "dependencies": {
    "@navikt/ds-css": "2.8.4",
    "@navikt/ft-kodeverk": "^2.0.8",
    "@navikt/ft-types": "^2.0.8",
    "@navikt/ft-ui-komponenter": "^2.0.10",
    "@navikt/ft-utils": "^2.0.9",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-intl": "6.3.2",
    "vis-data": "7.1.6",
    "vis-timeline": "7.7.2"
  },
  "peerDependencies": {
    "@navikt/ds-css": "2.6.0",
    "@navikt/ft-kodeverk": "1.x",
    "@navikt/ft-types": "1.x",
    "@navikt/ft-ui-komponenter": "1.x",
    "@navikt/ft-utils": "1.x",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-intl": "6.2.10",
    "vis-data": "7.1.4",
    "vis-timeline": "7.7.0"
  },
  "devDependencies": {
    "@storybook/react": "6.5.16",
    "cross-env": "7.0.3",
    "eslint": "8.36.0",
    "stylelint": "15.3.0",
    "typescript": "5.0.2",
    "vite": "4.2.1",
    "vitest": "0.29.7"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }

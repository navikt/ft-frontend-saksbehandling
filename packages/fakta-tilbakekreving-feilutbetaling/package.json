{
  "name": "@navikt/ft-fakta-tilbakekreving-feilutbetaling",
  "description": "Saksbehandler angir årsak til at feilutbetalingen har oppstått for hver enkelt periode",
  "version": "2.1.12",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7008"
  },
  "dependencies": {
    "@navikt/ds-css": "3.1.2",
    "@navikt/ds-react": "3.1.2",
    "@navikt/ft-form-hooks": "^4.0.30",
    "@navikt/ft-form-validators": "^2.0.25",
    "@navikt/ft-kodeverk": "^2.1.7",
    "@navikt/ft-types": "^2.1.8",
    "@navikt/ft-ui-komponenter": "^2.0.28",
    "@navikt/ft-utils": "^2.0.24",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-hook-form": "7.43.9",
    "react-intl": "6.4.2"
  },
  "peerDependencies": {
    "@navikt/ds-css": "2.8.11",
    "@navikt/ds-react": "2.8.11",
    "@navikt/ft-form-hooks": "4.x",
    "@navikt/ft-form-validators": "2.x",
    "@navikt/ft-kodeverk": "2.x",
    "@navikt/ft-types": "2.x",
    "@navikt/ft-ui-komponenter": "2.x",
    "@navikt/ft-utils": "2.x",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-hook-form": "7.43.9",
    "react-intl": "6.3.2"
  },
  "devDependencies": {
    "@navikt/ft-storybook-utils": "^2.0.30",
    "@navikt/ft-utils-test": "^2.0.20",
    "@storybook/addon-actions": "7.0.10",
    "@storybook/react": "7.0.10",
    "@storybook/react-vite": "7.0.10",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "cross-env": "7.0.3",
    "eslint": "8.40.0",
    "storybook": "7.0.10",
    "stylelint": "15.6.1",
    "typescript": "5.0.4",
    "vite": "4.3.5",
    "vitest": "0.31.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

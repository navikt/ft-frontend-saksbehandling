    "react-hook-form": "7.53.0",
    "react-intl": "6.7.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.umd.js"
    }
  },
  "devDependencies": {
    "eslint": "9.11.1",
    "typescript": "5.6.2",
    "vite": "5.4.8"

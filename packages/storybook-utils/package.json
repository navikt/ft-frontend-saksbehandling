  "license": "MIT",
  "type": "module",
  "private": true,
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "tsc": "tsc --pretty",
    "eslint": "eslint \"decorators/**/*.ts*\" --color",

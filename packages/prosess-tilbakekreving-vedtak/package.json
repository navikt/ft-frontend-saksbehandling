{
  "name": "@navikt/ft-prosess-tilbakekreving-vedtak",
  "description": "Vedtaksbilde hvor saksbehandler ferdigstiller vedtaksbrev, med mulighet for å legge til utdypende tekst. I noen tilfeller vil utdypende tekst være obligatorisk, for eksempel ved revurderingsbehandling.",
  "version": "2.2.17",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7011"
  },
  "dependencies": {
    "@navikt/aksel-icons": "5.11.2",
    "@navikt/ds-css": "5.11.2",
    "@navikt/ds-react": "5.11.2",
    "@navikt/ft-form-hooks": "^4.2.17",
    "@navikt/ft-form-validators": "^2.1.10",
    "@navikt/ft-kodeverk": "^2.2.10",
    "@navikt/ft-types": "^2.2.10",
    "@navikt/ft-ui-komponenter": "^2.1.17",
    "@navikt/ft-utils": "^2.1.10",
    "classnames": "2.3.2",
    "react": "18.2.0",
    "react-hook-form": "7.48.2",
    "react-intl": "6.5.5"
  },
  "peerDependencies": {
    "@navikt/aksel-icons": "3.1.2",
    "@navikt/ds-css": "2.8.11",
    "@navikt/ds-react": "2.8.11",
    "@navikt/ft-form-hooks": "4.x",
    "@navikt/ft-form-validators": "2.x",
    "@navikt/ft-kodeverk": "2.x",
    "@navikt/ft-types": "2.x",
    "@navikt/ft-ui-komponenter": "2.x",
    "@navikt/ft-utils": "2.x",
    "classnames": "2.3.2",
    "react": "18.2.0",
    "react-hook-form": "7.43.9",
    "react-intl": "6.3.2"
  },
  "devDependencies": {
    "@navikt/ft-storybook-utils": "^2.1.17",
    "@storybook/addon-actions": "7.6.0",
    "@storybook/react": "7.6.0",
    "@storybook/react-vite": "7.6.0",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
    "cross-env": "7.0.3",
    "eslint": "8.54.0",
    "storybook": "7.6.0",
    "stylelint": "15.11.0",
    "typescript": "5.3.2",
    "vite": "5.0.8",
    "vitest": "1.0.4"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

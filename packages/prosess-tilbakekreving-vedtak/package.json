{
  "name": "@navikt/ft-prosess-tilbakekreving-vedtak",
  "description": "Vedtaksbilde hvor saksbehandler ferdigstiller vedtaksbrev, med mulighet for å legge til utdypende tekst. I noen tilfeller vil utdypende tekst være obligatorisk, for eksempel ved revurderingsbehandling.",
  "version": "2.1.27",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7011"
  },
  "dependencies": {
    "@navikt/aksel-icons": "4.7.2",
    "@navikt/ds-css": "4.8.0",
    "@navikt/ds-react": "4.7.2",
    "@navikt/ft-form-hooks": "^4.1.7",
    "@navikt/ft-form-validators": "^2.0.39",
    "@navikt/ft-kodeverk": "^2.1.19",
    "@navikt/ft-types": "^2.1.21",
    "@navikt/ft-ui-komponenter": "^2.0.42",
    "@navikt/ft-utils": "^2.0.38",
    "classnames": "2.3.2",
    "react": "18.2.0",
    "react-hook-form": "7.45.2",
    "react-intl": "6.4.4"
  },
  "peerDependencies": {
    "@navikt/aksel-icons": "3.1.2",
    "@navikt/ds-css": "2.8.11",
    "@navikt/ds-react": "2.8.11",
    "@navikt/ft-form-hooks": "4.x",
    "@navikt/ft-form-validators": "2.x",
    "@navikt/ft-kodeverk": "2.x",
    "@navikt/ft-types": "2.x",
    "@navikt/ft-ui-komponenter": "2.x",
    "@navikt/ft-utils": "2.x",
    "classnames": "2.3.2",
    "react": "18.2.0",
    "react-hook-form": "7.43.9",
    "react-intl": "6.3.2"
  },
  "devDependencies": {
    "@navikt/ft-storybook-utils": "^2.0.45",
    "@storybook/addon-actions": "7.1.1",
    "@storybook/react": "7.1.1",
    "@storybook/react-vite": "7.1.1",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "cross-env": "7.0.3",
    "eslint": "8.46.0",
    "storybook": "7.1.1",
    "stylelint": "15.10.2",
    "typescript": "5.1.6",
    "vite": "4.4.7",
    "vitest": "0.33.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

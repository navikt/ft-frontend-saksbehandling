{
  "name": "@navikt/ft-prosess-tilbakekreving-vedtak",
  "description": "Vedtaksbilde hvor saksbehandler ferdigstiller vedtaksbrev, med mulighet for å legge til utdypende tekst. I noen tilfeller vil utdypende tekst være obligatorisk, for eksempel ved revurderingsbehandling.",
  "version": "1.1.3",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --colors",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\"",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\"",
    "less:lint": "stylelint \"src/**/*.less\"",
    "dev": "cross-env node dev/webpack.dev.js",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --target esnext --emitDeclarationOnly --downlevelIteration --resolveJsonModule --moduleResolution node --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules",
    "storybook": "cross-env start-storybook -p 7011"
  },
  "dependencies": {
    "@navikt/ds-css": "^1.3.0",
    "@navikt/ds-react": "^1.3.0",
    "@navikt/ft-form-hooks": "^2.0.3",
    "@navikt/ft-form-validators": "^1.0.30",
    "@navikt/ft-kodeverk": "^1.0.18",
    "@navikt/ft-types": "^1.0.28",
    "@navikt/ft-ui-komponenter": "^1.1.3",
    "@navikt/ft-utils": "^1.1.1",
    "classnames": "2.3.2",
    "nav-frontend-core": "6.0.1",
    "react": "18.2.0",
    "react-hook-form": "7.35.0",
    "react-intl": "6.1.1"
  },
  "peerDependencies": {
    "@navikt/ds-css": "^1.3.0",
    "@navikt/ds-react": "^1.3.0",
    "@navikt/ft-form-hooks": "^1.x",
    "@navikt/ft-form-validators": "^1.x",
    "@navikt/ft-kodeverk": "^1.x",
    "@navikt/ft-types": "^1.x",
    "@navikt/ft-ui-komponenter": "^1.x",
    "@navikt/ft-utils": "^1.x",
    "classnames": "2.x",
    "nav-frontend-core": "6.x",
    "react": "18.x",
    "react-hook-form": "7.x",
    "react-intl": "6.1.1"
  },
  "devDependencies": {
    "@navikt/ft-storybook-utils": "^1.0.7",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "typescript": "4.8.3",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "3.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  }
}

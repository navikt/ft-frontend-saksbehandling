{
  "name": "@navikt/ft-prosess-tilbakekreving-vedtak",
  "description": "Vedtaksbilde hvor saksbehandler ferdigstiller vedtaksbrev, med mulighet for å legge til utdypende tekst. I noen tilfeller vil utdypende tekst være obligatorisk, for eksempel ved revurderingsbehandling.",
  "version": "1.0.20",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --colors",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\"",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\"",
    "less:lint": "stylelint \"src/**/*.less\"",
    "dev": "cross-env node dev/webpack.dev.js",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --target esnext --emitDeclarationOnly --downlevelIteration --resolveJsonModule --moduleResolution node --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules",
    "storybook": "cross-env start-storybook -p 7011"
  },
  "dependencies": {
    "@navikt/ft-form-hooks": "^1.0.28",
    "@navikt/ft-form-validators": "^1.0.18",
    "@navikt/ft-kodeverk": "^1.0.11",
    "@navikt/ft-types": "^1.0.17",
    "@navikt/ft-ui-komponenter": "^1.0.26",
    "@navikt/ft-utils": "^1.0.23",
    "classnames": "2.3.1",
    "nav-frontend-core": "6.0.1",
    "nav-frontend-ekspanderbartpanel": "4.0.3",
    "nav-frontend-ekspanderbartpanel-style": "2.0.1",
    "nav-frontend-js-utils": "1.0.20",
    "nav-frontend-knapper": "3.1.2",
    "nav-frontend-knapper-style": "2.1.1",
    "nav-frontend-typografi": "4.0.1",
    "nav-frontend-typografi-style": "2.0.1",
    "react": "17.0.2",
    "react-hook-form": "7.34.0",
    "react-intl": "6.0.5"
  },
  "peerDependencies": {
    "@navikt/ft-form-hooks": "^1.0.21",
    "@navikt/ft-form-validators": "^1.0.8",
    "@navikt/ft-kodeverk": "^1.0.6",
    "@navikt/ft-types": "^1.0.9",
    "@navikt/ft-ui-komponenter": "^1.0.15",
    "@navikt/ft-utils": "^1.0.13",
    "classnames": "2.3.1",
    "nav-frontend-core": "6.0.1",
    "nav-frontend-ekspanderbartpanel": "4.0.3",
    "nav-frontend-ekspanderbartpanel-style": "2.0.1",
    "nav-frontend-js-utils": "1.0.20",
    "nav-frontend-knapper": "3.1.2",
    "nav-frontend-knapper-style": "2.1.1",
    "nav-frontend-typografi": "4.0.1",
    "nav-frontend-typografi-style": "2.0.1",
    "react": "17.0.2",
    "react-hook-form": "7.34.0",
    "react-intl": "6.0.5"
  },
  "devDependencies": {
    "@navikt/ft-storybook-utils": "^1.0.4",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/react": "12.1.4",
    "@testing-library/user-event": "13.5.0",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "typescript": "4.7.4",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "3.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  }
}

{
  "name": "@navikt/ft-utils",
  "version": "2.0.6",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules"
  },
  "dependencies": {
    "@navikt/ft-kodeverk": "^2.0.6",
    "@navikt/ft-types": "^2.0.6",
    "dayjs": "1.11.7",
    "react-intl": "6.2.10"
  },
  "peerDependencies": {
    "@navikt/ft-kodeverk": "1.x",
    "@navikt/ft-types": "1.x",
    "dayjs": "1.11.7",
    "react-intl": "6.2.10"
  },
  "devDependencies": {
    "clean-webpack-plugin": "4.0.0",
    "eslint": "8.36.0",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vitest": "0.29.2",
    "webpack": "5.76.1",
    "webpack-cli": "5.0.1",
    "webpack-node-externals": "3.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  }
}

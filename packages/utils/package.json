{
  "name": "@navikt/ft-utils",
  "version": "1.0.0-beta.1",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
      "dist",
      "index.js",
      "package.json"
  ],
  "scripts": {
    "build-webpack": "cross-env NODE_OPTIONS='--openssl-legacy-provider' webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly --downlevelIteration --resolveJsonModule --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack & yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules",
    "test": "jest --passWithNoTests",
    "ts-check": "tsc --pretty"
  },
  "dependencies": {
    "@navikt/ft-kodeverk": "1.0.0-beta.1",
    "@navikt/ft-types": "1.0.0-beta.1",
    "dayjs": "1.11.0",
    "moment": "2.29.1",
    "react-intl": "5.24.7"
  },
  "peerDependencies": {
    "@navikt/ft-kodeverk": "1.0.0-beta.1",
    "@navikt/ft-types": "1.0.0-beta.1",
    "dayjs": "1.11.0",
    "moment": "2.29.1",
    "react-intl": "5.24.7"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "typescript": "4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
      "type": "git",
      "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  }
}

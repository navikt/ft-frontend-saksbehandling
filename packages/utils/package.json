{
  "name": "@navikt/ft-utils",
  "version": "1.1.9",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --colors",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\"",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\"",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly --downlevelIteration --resolveJsonModule --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules"
  },
  "dependencies": {
    "@navikt/ft-kodeverk": "^1.0.23",
    "@navikt/ft-types": "^1.0.34",
    "dayjs": "1.11.6",
    "react-intl": "6.2.5"
  },
  "peerDependencies": {
    "@navikt/ft-kodeverk": "^1.x",
    "@navikt/ft-types": "^1.x",
    "dayjs": "1.11.6",
    "react-intl": "6.2.5"
  },
  "devDependencies": {
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "typescript": "4.9.3",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.0",
    "webpack-node-externals": "3.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  }
}

    "@navikt/ft-kodeverk": "^1.0.24",
    "@navikt/ft-konstanter": "^1.0.17",
    "@navikt/ft-types": "^1.0.37",
    "@navikt/ft-ui-komponenter": "^1.3.6",
    "@navikt/ft-utils": "^1.1.12",
    "core-js": "3.27.1",
    "dayjs": "1.11.7",
    "nav-frontend-core": "6.0.1",
    "nav-frontend-js-utils": "1.0.20",
    "nav-frontend-tabs": "^2.0.2",
    "nav-frontend-tabs-style": "^2.0.1",
    "nav-frontend-typografi-style": "^2.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.41.5",
    "react-intl": "6.2.5"
  },
  "peerDependencies": {
    "@navikt/ds-css": "2.0.12",
    "@navikt/ds-react": "2.0.12",
    "@navikt/ft-form-hooks": "^3.x",
    "@navikt/ft-form-validators": "^1.x",
    "@navikt/ft-kodeverk": "^1.x",
    "@navikt/ft-konstanter": "^1.x",

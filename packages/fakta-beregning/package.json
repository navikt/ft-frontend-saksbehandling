{
  "name": "@navikt/ft-fakta-beregning",
  "description": "Faktaavklaringer rundt aktiviteter og andeler som skal bidra til beregningsgrunnlaget",
  "version": "2.2.10",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --colors",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\"",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\"",
    "less:lint": "stylelint \"src/**/*.less\"",
    "dev": "cross-env node dev/webpack.dev.js",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly --downlevelIteration --resolveJsonModule --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules",
    "storybook": "cross-env start-storybook -p 7001"
  },
  "dependencies": {
    "@navikt/ds-css": "^1.3.5",
    "@navikt/ds-react": "^1.3.5",
    "@navikt/ft-form-hooks": "^2.0.7",
    "@navikt/ft-form-validators": "^1.0.33",
    "@navikt/ft-kodeverk": "^1.0.19",
    "@navikt/ft-konstanter": "^1.0.14",
    "@navikt/ft-types": "^1.0.30",
    "@navikt/ft-ui-komponenter": "^1.1.7",
    "@navikt/ft-utils": "^1.1.4",
    "core-js": "3.25.4",
    "dayjs": "1.11.5",
    "nav-frontend-core": "6.0.1",
    "nav-frontend-tabs": "^2.0.2",
    "nav-frontend-tabs-style": "^2.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.36.1",
    "react-intl": "6.1.2"
  },
  "peerDependencies": {
    "@navikt/ds-css": "^1.3.5",
    "@navikt/ds-react": "^1.3.5",
    "@navikt/ft-form-hooks": "^2.x",
    "@navikt/ft-form-validators": "^1.x",
    "@navikt/ft-kodeverk": "^1.x",
    "@navikt/ft-konstanter": "^1.x",
    "@navikt/ft-types": "^1.x",
    "@navikt/ft-ui-komponenter": "1.x",
    "@navikt/ft-utils": "^1.x",
    "core-js": "3.25.4",
    "dayjs": "1.11.5",
    "nav-frontend-core": "6.0.1",
    "nav-frontend-tabs": "^2.0.2",
    "nav-frontend-tabs-style": "^2.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.36.1",
    "react-intl": "6.1.2"
  },
  "devDependencies": {
    "@navikt/ft-storybook-utils": "^1.0.7",
    "@navikt/ft-utils-test": "^1.1.2",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "sinon": "14.0.0",
    "typescript": "4.8.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "3.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",

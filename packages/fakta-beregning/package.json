{
  "name": "@navikt/ft-fakta-beregning",
  "description": "Faktaavklaringer rundt aktiviteter og andeler som skal bidra til beregningsgrunnlaget",
  "version": "12.1.20",
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest --project=jsdom",
    "test:watch": "vitest --project=jsdom --watch=true",
    "test:browser": "vitest --project=browser",
    "test:browser:watch": "vitest --project=browser --watch=true",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules"
  },
  "dependencies": {
    "@navikt/aksel-icons": "7.31.0",
    "@navikt/ds-css": "7.31.0",
    "@navikt/ds-react": "7.31.0",
    "@navikt/ft-form-hooks": "workspace:^",
    "@navikt/ft-form-validators": "workspace:^",
    "@navikt/ft-ui-komponenter": "workspace:^",
    "@navikt/ft-utils": "workspace:^",
    "core-js": "3.45.1",
    "dayjs": "1.11.18",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "7.64.0",
    "react-intl": "7.1.11"
  },
  "peerDependencies": {
    "@navikt/aksel-icons": "7.x",
    "@navikt/ds-css": "7.x",
    "@navikt/ds-react": "7.x",
    "@navikt/ft-form-hooks": "5.x",
    "@navikt/ft-form-validators": "2.x",
    "@navikt/ft-kodeverk": "3.x",
    "@navikt/ft-plattform-komponenter": "5.x",
    "@navikt/ft-types": "3.x",
    "@navikt/ft-ui-komponenter": "2.x",
    "@navikt/ft-utils": "2.x",
    "core-js": "3.37.1",
    "dayjs": "1.11.x",
    "dayjs-plugin-utc": "0.1.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.x",
    "react-intl": "6.x"
  },
  "devDependencies": {
    "@navikt/ft-config-eslint": "workspace:^",
    "@navikt/ft-config-typescript": "workspace:^",
    "@navikt/ft-config-vite": "workspace:^",
    "@navikt/ft-frontend-storybook-utils": "workspace:^",
    "@navikt/ft-frontend-utils-test": "workspace:^",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "eslint": "9.36.0",
    "stylelint": "16.24.0",
    "typescript": "5.9.2",
    "vite": "7.1.7",
    "vitest": "3.2.4"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

{
  "name": "@navikt/ft-sak-risikoklassifisering",
  "description": "Viser risikoklassifisering for valgt behandling. Viser en av tre komponenter avhengig av: Om ingen klassifisering er utført, om klassifisering er utført og ingen faresignaler er funnet og om klassifisering er utført og faresignaler er funnet",
  "version": "1.3.19",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "less:lint": "stylelint \"src/**/*.less\"",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly --downlevelIteration --resolveJsonModule --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules",
    "storybook": "start-storybook --quiet -p 7019"
  },
  "dependencies": {
    "@navikt/ds-css": "2.3.1",
    "@navikt/ds-react": "2.1.7",
    "@navikt/ft-form-hooks": "^3.3.5",
    "@navikt/ft-form-validators": "^1.0.52",
    "@navikt/ft-kodeverk": "^1.0.26",
    "@navikt/ft-types": "^1.1.2",
    "@navikt/ft-ui-komponenter": "^1.3.15",
    "@navikt/ft-utils": "^1.1.20",
    "react": "18.2.0",
    "react-collapse": "5.1.1",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.1",
    "react-intl": "6.2.8"
  },
  "peerDependencies": {
    "@navikt/ds-css": "2.3.1",
    "@navikt/ds-react": "2.1.7",
    "@navikt/ft-form-hooks": "3.x",
    "@navikt/ft-form-validators": "1.x",
    "@navikt/ft-kodeverk": "^1.x",
    "@navikt/ft-types": "^1.x",
    "@navikt/ft-ui-komponenter": "1.x",
    "@navikt/ft-utils": "1.x",
    "react": "18.2.0",
    "react-collapse": "5.1.1",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.1",
    "react-intl": "6.2.8"
  },
  "devDependencies": {
    "@navikt/ft-storybook-utils": "^1.0.7",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": "./dist/index.js",
    "./dist/style.css": "./dist/style.css"
  }
}

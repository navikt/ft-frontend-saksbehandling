{
  "name": "@navikt/ft-sak-risikoklassifisering",
  "description": "Viser risikoklassifisering for valgt behandling. Viser en av tre komponenter avhengig av: Om ingen klassifisering er utført, om klassifisering er utført og ingen faresignaler er funnet og om klassifisering er utført og faresignaler er funnet",
  "version": "2.1.0",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7019"
  },
  "dependencies": {
    "@navikt/ds-css": "2.8.12",
    "@navikt/ds-react": "2.8.12",
    "@navikt/ft-form-hooks": "^4.0.18",
    "@navikt/ft-form-validators": "^2.0.16",
    "@navikt/ft-kodeverk": "^2.1.0",
    "@navikt/ft-types": "^2.1.0",
    "@navikt/ft-ui-komponenter": "^2.0.16",
    "@navikt/ft-utils": "^2.0.15",
    "react": "18.2.0",
    "react-collapse": "5.1.1",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.9",
    "react-intl": "6.4.0"
  },
  "peerDependencies": {
    "@navikt/ds-css": "2.8.11",
    "@navikt/ds-react": "2.8.11",
    "@navikt/ft-form-hooks": "4.x",
    "@navikt/ft-form-validators": "2.x",
    "@navikt/ft-kodeverk": "2.x",
    "@navikt/ft-types": "2.x",
    "@navikt/ft-ui-komponenter": "2.x",
    "@navikt/ft-utils": "2.x",
    "react": "18.2.0",
    "react-collapse": "5.1.1",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.9",
    "react-intl": "6.3.2"
  },
  "devDependencies": {
    "@navikt/ft-storybook-utils": "^2.0.18",
    "@storybook/addon-actions": "7.0.5",
    "@storybook/react": "7.0.5",
    "@storybook/react-vite": "7.0.5",
    "@storybook/testing-react": "2.0.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "cross-env": "7.0.3",
    "eslint": "8.38.0",
    "storybook": "7.0.5",
    "stylelint": "15.5.0",
    "typescript": "5.0.4",
    "vite": "4.2.2",
    "vitest": "0.30.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

{
  "name": "@navikt/ft-form-hooks",
  "description": "Overbygg over react-hook-form. Tilbyr form-komponenter som radioknapp og textarea",
  "version": "4.0.15",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7003"
  },
  "dependencies": {
    "@navikt/ds-css": "2.8.12",
    "@navikt/ds-react": "2.8.12",
    "@navikt/ft-form-validators": "^2.0.13",
    "@navikt/ft-types": "^2.0.11",
    "@navikt/ft-ui-komponenter": "^2.0.13",
    "@navikt/ft-utils": "^2.0.12",
    "classnames": "2.3.2",
    "dayjs": "1.11.7",
    "lodash.throttle": "4.1.1",
    "react": "18.2.0",
    "react-hook-form": "7.43.9"
  },
  "peerDependencies": {
    "@navikt/ds-css": "2.8.11",
    "@navikt/ds-react": "2.8.11",
    "@navikt/ft-form-validators": "2.x",
    "@navikt/ft-types": "2.x",
    "@navikt/ft-ui-komponenter": "2.x",
    "@navikt/ft-utils": "2.x",
    "classnames": "2.3.2",
    "dayjs": "1.11.7",
    "lodash.throttle": "4.1.1",
    "react": "18.2.0",
    "react-hook-form": "7.43.9"
  },
  "devDependencies": {
    "@navikt/ft-utils-test": "^2.0.12",
    "@storybook/addon-actions": "7.0.4",
    "@storybook/react": "7.0.4",
    "@storybook/react-vite": "7.0.4",
    "@storybook/testing-react": "2.0.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "cross-env": "7.0.3",
    "eslint": "8.38.0",
    "storybook": "7.0.4",
    "stylelint": "15.4.0",
    "typescript": "5.0.4",
    "vite": "4.2.1",
    "vitest": "0.29.8"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

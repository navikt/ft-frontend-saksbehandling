{
  "name": "@navikt/ft-form-hooks",
  "description": "Overbygg over react-hook-form. Tilbyr form-komponenter som radioknapp og textarea",
  "version": "9.1.0",
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest --project=jsdom",
    "test:watch": "vitest --project=jsdom --watch=true",
    "test:browser": "vitest --project=browser",
    "test:browser:watch": "vitest --project=browser --watch=true",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules"
  },
  "dependencies": {
    "@navikt/aksel-icons": "7.28.0",
    "@navikt/ds-css": "7.28.0",
    "@navikt/ds-react": "7.28.0",
    "@navikt/ft-ui-komponenter": "workspace:^",
    "@navikt/ft-utils": "workspace:^",
    "dayjs": "1.11.13"
  },
  "peerDependencies": {
    "@navikt/aksel-icons": "7.x",
    "@navikt/ds-css": "7.x",
    "@navikt/ds-react": "7.x",
    "@navikt/ft-form-validators": "4.x",
    "@navikt/ft-ui-komponenter": "6.x",
    "@navikt/ft-utils": "3.x",
    "dayjs": "1.11.x",
    "react": "19.1.x",
    "react-hook-form": "7.x"
  },
  "devDependencies": {
    "@navikt/ft-config-eslint": "workspace:^",
    "@navikt/ft-config-typescript": "workspace:^",
    "@navikt/ft-config-vite": "workspace:^",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "eslint": "9.33.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "7.62.0",
    "stylelint": "16.23.1",
    "typescript": "5.9.2",
    "vite": "7.1.2",
    "vitest": "3.2.4"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

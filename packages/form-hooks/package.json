{
  "name": "@navikt/ft-form-hooks",
  "description": "Overbygg over react-hook-form. Tilbyr form-komponenter som radioknapp og textarea",
  "version": "4.0.0",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules",
    "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook --quiet -p 7003"
  },
  "dependencies": {
    "@navikt/ds-css": "2.4.3",
    "@navikt/ds-react": "2.1.7",
    "@navikt/ft-form-validators": "^2.0.0",
    "@navikt/ft-types": "^2.0.0",
    "@navikt/ft-ui-komponenter": "^2.0.0",
    "@navikt/ft-utils": "^2.0.0",
    "classnames": "2.3.2",
    "dayjs": "1.11.7",
    "lodash.throttle": "4.1.1",
    "react": "18.2.0",
    "react-hook-form": "7.43.1"
  },
  "peerDependencies": {
    "@navikt/ds-css": "2.4.3",
    "@navikt/ds-react": "2.1.7",
    "@navikt/ft-form-validators": "^1.x",
    "@navikt/ft-types": "^1.x",
    "@navikt/ft-ui-komponenter": "^1.x",
    "@navikt/ft-utils": "^1.x",
    "classnames": "2.3.2",
    "dayjs": "1.11.7",
    "lodash.throttle": "4.1.1",
    "react": "18.2.0",
    "react-hook-form": "7.43.1"
  },
  "devDependencies": {
    "@navikt/ft-utils-test": "^2.0.0",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "typescript": "4.9.5",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-node-externals": "3.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

{
  "name": "@navikt/ft-form-hooks",
  "description": "Overbygg over react-hook-form. Tilbyr form-komponenter som radioknapp og textarea",
  "version": "3.1.9",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --colors",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\"",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\"",
    "less:lint": "stylelint \"src/**/*.less\"",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly --downlevelIteration --resolveJsonModule --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules",
    "storybook": "cross-env start-storybook -p 7003"
  },
  "dependencies": {
    "@navikt/ds-css": "2.0.2",
    "@navikt/ds-react": "1.5.9",
    "@navikt/ft-form-validators": "^1.0.38",
    "@navikt/ft-types": "^1.0.34",
    "@navikt/ft-ui-komponenter": "^1.2.6",
    "@navikt/ft-utils": "^1.1.9",
    "classnames": "2.3.2",
    "dayjs": "1.11.6",
    "lodash.throttle": "4.1.1",
    "react": "18.2.0",
    "react-hook-form": "7.39.4"
  },
  "peerDependencies": {
    "@navikt/ds-css": "2.0.2",
    "@navikt/ds-react": "1.5.9",
    "@navikt/ft-form-validators": "^1.x",
    "@navikt/ft-types": "^1.x",
    "@navikt/ft-ui-komponenter": "^1.x",
    "@navikt/ft-utils": "^1.x",
    "classnames": "2.3.2",
    "dayjs": "1.11.6",
    "lodash.throttle": "4.1.1",
    "react": "18.2.0",
    "react-hook-form": "7.39.4"
  },
  "devDependencies": {
    "@navikt/ft-utils-test": "^1.1.8",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "typescript": "4.9.3",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.0",
    "webpack-node-externals": "3.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": "./dist/index.js",
    "./dist/style.css": "./dist/style.css"
  }
}

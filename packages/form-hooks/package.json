{
  "name": "@navikt/ft-form-hooks",
  "description": "Overbygg over react-hook-form. Tilbyr form-komponenter som radioknapp og textarea",
  "version": "5.2.9",
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7003"
  },
  "dependencies": {
    "@navikt/ft-form-validators": "^2.5.7",
    "@navikt/ft-types": "^2.5.11",
    "@navikt/ft-ui-komponenter": "^2.6.5",
    "@navikt/ft-utils": "^2.5.4",
    "classnames": "2.5.1",
    "dayjs": "1.11.13",
    "lodash.throttle": "4.1.1"
  },
  "peerDependencies": {
    "@navikt/aksel-icons": "7.x",
    "@navikt/ds-css": "7.x",
    "@navikt/ds-react": "7.x",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.x"
  },
  "devDependencies": {
    "@navikt/aksel-icons": "7.6.0",
    "@navikt/ds-css": "7.6.0",
    "@navikt/ds-react": "7.6.0",
    "@navikt/ft-frontend-utils-test": "workspace:^",
    "@storybook/addon-actions": "8.4.7",
    "@storybook/react": "8.4.7",
    "@storybook/react-vite": "8.4.7",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "14.5.2",
    "cross-env": "7.0.3",
    "eslint": "9.16.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.53.2",
    "storybook": "8.4.7",
    "stylelint": "16.11.0",
    "typescript": "5.7.2",
    "vite": "5.4.11",
    "vitest": "2.1.8"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

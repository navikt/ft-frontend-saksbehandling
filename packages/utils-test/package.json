{
  "name": "@navikt/ft-utils-test",
  "version": "1.2.4",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --target esnext --declaration --emitDeclarationOnly --downlevelIteration --moduleResolution node --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules"
  },
  "dependencies": {
    "@navikt/ft-kodeverk": "^1.0.26",
    "@testing-library/jest-dom": "5.16.5",
    "axios-mock-adapter": "1.21.2",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "raf": "3.4.1",
    "react": "18.2.0",
    "react-intl": "6.2.8",
    "sinon": "15.0.1",
    "typescript": "4.9.5",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-node-externals": "3.0.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "typescript": "4.9.5",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-node-externals": "3.0.0"
  }
}

    "@navikt/ft-konstanter": "^2.0.0",
    "@navikt/ft-plattform-komponenter": "^2.0.0",
    "@navikt/ft-types": "^2.0.0",
    "@navikt/ft-ui-komponenter": "^2.0.0",
    "@navikt/ft-utils": "^2.0.0",
    "classnames": "2.3.2",
    "react": "18.2.0",
    "react-intl": "6.2.8"
  },
  "peerDependencies": {
    "@navikt/ds-css": "2.1.7",
    "@navikt/ds-icons": "2.4.3",
    "@navikt/ds-react": "2.1.7",
    "@navikt/ft-kodeverk": "^1.x",
    "@navikt/ft-konstanter": "^1.x",
    "@navikt/ft-plattform-komponenter": "1.x",
    "@navikt/ft-types": "^1.x",
    "@navikt/ft-ui-komponenter": "1.x",
    "@navikt/ft-utils": "1.x",
    "classnames": "2.3.2",
    "react": "18.2.0",
    "react-intl": "6.2.8"
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.5.16",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }

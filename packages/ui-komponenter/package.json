{
  "name": "@navikt/ft-ui-komponenter",
  "description": "Pakke som tilbyr generelle komponenter som grid og tabell",
  "version": "1.3.12",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "test": "jest --colors",
    "test:coverage": "jest --coverage --colors",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "less:lint": "stylelint \"src/**/*.less\"",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly --downlevelIteration --resolveJsonModule --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules",
    "storybook": "start-storybook --quiet -p 7006"
  },
  "dependencies": {
    "@navikt/ds-css": "2.1.5",
    "@navikt/ds-react": "2.1.5",
    "@navikt/fnrvalidator": "1.3.3",
    "@navikt/ft-utils": "^1.1.17",
    "classnames": "2.3.2",
    "date-fns": "^2.29.3",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-day-picker": "8.5.1",
    "react-intl": "6.2.7",
    "react-modal": "3.16.1"
  },
  "peerDependencies": {
    "@navikt/ds-css": "2.1.5",
    "@navikt/ds-react": "2.1.5",
    "@navikt/fnrvalidator": "1.3.3",
    "@navikt/ft-utils": "^1.x",
    "classnames": "2.3.2",
    "date-fns": "^2.29.3",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-day-picker": "8.5.1",
    "react-intl": "6.2.7",
    "react-modal": "3.16.1"
  },
  "devDependencies": {
    "@navikt/ft-utils-test": "^1.2.3",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "sinon": "15.0.1",
    "typescript": "4.9.5",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-node-externals": "3.0.0"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": "./dist/index.js",
    "./dist/style.css": "./dist/style.css"
  }
}

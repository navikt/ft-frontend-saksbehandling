{
  "name": "@navikt/ft-ui-komponenter",
  "description": "Pakke som tilbyr generelle komponenter som grid og tabell",
  "version": "3.2.2",
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules"
  },
  "dependencies": {
    "@navikt/aksel-icons": "7.17.0",
    "@navikt/ds-css": "7.17.0",
    "@navikt/ds-react": "7.17.0",
    "@navikt/ft-utils": "workspace:^",
    "classnames": "2.5.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-intl": "7.1.9",
    "react-modal": "3.16.3"
  },
  "peerDependencies": {
    "@navikt/aksel-icons": "7.x",
    "@navikt/ds-css": "7.x",
    "@navikt/ds-react": "7.x",
    "@navikt/ft-utils": "2.x",
    "classnames": "2.5.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-intl": "6.x",
    "react-modal": "3.16.1"
  },
  "devDependencies": {
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "14.6.1",
    "cross-env": "7.0.3",
    "eslint": "9.22.0",
    "stylelint": "16.16.0",
    "typescript": "5.8.2",
    "vite": "6.2.2",
    "vitest": "3.0.9"
  },
  "resolutions": {
    "@types/react": "19.0.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-test-renderer": "19.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

{
  "name": "@navikt/ft-ui-komponenter",
  "description": "Pakke som tilbyr generelle komponenter som grid og tabell",
  "version": "6.2.9",
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest --project=jsdom",
    "test:watch": "vitest --project=jsdom --watch=true",
    "test:browser": "vitest --project=browser",
    "test:browser:watch": "vitest --project=browser --watch=true",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules"
  },
  "dependencies": {
    "@navikt/aksel-icons": "7.32.1",
    "@navikt/ds-react": "7.32.1",
    "@navikt/ft-utils": "workspace:^",
    "classnames": "2.5.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-intl": "7.1.14"
  },
  "peerDependencies": {
    "@navikt/aksel-icons": "7.x",
    "@navikt/ds-css": "7.x",
    "@navikt/ds-react": "7.x",
    "@navikt/ft-utils": "2.x",
    "classnames": "2.5.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-intl": "6.x",
    "react-modal": "3.16.1"
  },
  "devDependencies": {
    "@navikt/ft-config-eslint": "workspace:^",
    "@navikt/ft-config-typescript": "workspace:^",
    "@navikt/ft-config-vite": "workspace:^",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "eslint": "9.37.0",
    "stylelint": "16.25.0",
    "typescript": "5.9.3",
    "vite": "7.1.10",
    "vitest": "3.2.4"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

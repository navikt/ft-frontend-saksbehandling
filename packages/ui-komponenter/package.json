{
  "name": "@navikt/ft-ui-komponenter",
  "type": "module",
  "description": "Pakke som tilbyr generelle komponenter som grid og tabell",
  "version": "1.3.15",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "dev": "vite build --watch",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules",
    "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook --quiet -p 7006"
  },
  "dependencies": {
    "@navikt/ds-css": "2.1.7",
    "@navikt/ds-react": "2.1.7",
    "@navikt/fnrvalidator": "1.3.3",
    "@navikt/ft-utils": "1.1.20",
    "classnames": "2.3.2",
    "date-fns": "2.29.3",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-day-picker": "8.5.1",
    "react-intl": "6.2.8",
    "react-modal": "3.16.1"
  },
  "peerDependencies": {
    "@navikt/ds-css": "2.1.7",
    "@navikt/ds-react": "2.1.7",
    "@navikt/fnrvalidator": "1.3.3",
    "@navikt/ft-utils": "1.x",
    "classnames": "2.3.2",
    "date-fns": "2.29.3",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-day-picker": "8.5.1",
    "react-intl": "6.2.8",
    "react-modal": "3.16.1"
  },
  "devDependencies": {
    "@navikt/ft-utils-test": "1.2.5",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "sinon": "15.0.1",
    "typescript": "4.9.5",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-node-externals": "3.0.0"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

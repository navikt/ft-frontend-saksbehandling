{
  "name": "@navikt/ft-ui-komponenter",
  "description": "Pakke som tilbyr generelle komponenter som grid og tabell",
  "version": "1.1.7",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --colors",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\"",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\"",
    "less:lint": "stylelint \"src/**/*.less\"",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly --downlevelIteration --resolveJsonModule --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules",
    "storybook": "cross-env start-storybook -p 7006"
  },
  "dependencies": {
    "@navikt/ds-css": "^1.3.5",
    "@navikt/ds-react": "^1.3.5",
    "@navikt/fnrvalidator": "1.3.3",
    "@navikt/ft-utils": "^1.1.4",
    "classnames": "2.3.2",
    "date-fns": "^2.29.3",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-day-picker": "7.4.10",
    "react-intl": "6.1.2",
    "react-modal": "3.15.1"
  },
  "peerDependencies": {
    "@navikt/ds-css": "^1.3.5",
    "@navikt/ds-react": "^1.3.5",
    "@navikt/fnrvalidator": "1.3.3",
    "@navikt/ft-utils": "^1.x",
    "classnames": "2.3.2",
    "date-fns": "^2.29.3",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-day-picker": "7.4.10",
    "react-intl": "6.1.2",
    "react-modal": "3.15.1"
  },
  "devDependencies": {
    "@navikt/ft-utils-test": "^1.1.2",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "sinon": "14.0.0",
    "typescript": "4.8.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "3.0.0"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": "./dist/index.js",
    "./dist/style.css": "./dist/style.css"
  }
}

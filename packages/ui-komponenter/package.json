{
  "name": "@navikt/ft-ui-komponenter",
  "description": "Pakke som tilbyr generelle komponenter som grid og tabell",
  "version": "2.2.4",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7006"
  },
  "dependencies": {
    "@navikt/aksel-icons": "5.18.1",
    "@navikt/ds-css": "5.18.1",
    "@navikt/ds-react": "5.18.1",
    "@navikt/ft-utils": "^2.2.3",
    "classnames": "2.5.1",
    "react": "18.2.0",
    "react-intl": "6.6.2",
    "react-modal": "3.16.1"
  },
  "peerDependencies": {
    "@navikt/aksel-icons": "3.1.2",
    "@navikt/ds-css": "2.8.11",
    "@navikt/ds-react": "2.8.11",
    "@navikt/ft-utils": "2.x",
    "classnames": "2.3.2",
    "react": "18.2.0",
    "react-intl": "6.3.2",
    "react-modal": "3.16.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "7.6.15",
    "@storybook/react": "7.6.17",
    "@storybook/react-vite": "7.6.15",
    "@testing-library/react": "14.2.1",
    "@testing-library/user-event": "14.5.2",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "storybook": "7.6.17",
    "stylelint": "15.11.0",
    "typescript": "5.3.3",
    "vite": "5.1.1",
    "vitest": "1.2.2"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

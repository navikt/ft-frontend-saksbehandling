{
  "name": "@navikt/ft-sak-infosider",
  "description": "Diverse info- og feil-sider",
  "version": "1.3.9",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "test": "jest --colors",
    "test:coverage": "jest --coverage --colors",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "less:lint": "stylelint \"src/**/*.less\"",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly --downlevelIteration --resolveJsonModule --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules",
    "storybook": "start-storybook --quiet -p 7020"
  },
  "dependencies": {
    "@navikt/ds-css": "2.1.5",
    "@navikt/ds-react": "2.1.5",
    "@navikt/ft-utils": "^1.1.17",
    "react": "18.2.0",
    "react-intl": "6.2.7"
  },
  "peerDependencies": {
    "@navikt/ds-css": "2.1.5",
    "@navikt/ds-react": "2.1.5",
    "@navikt/ft-utils": "1.x",
    "react": "18.2.0",
    "react-intl": "6.2.7"
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.5.16",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": "./dist/index.js",
    "./dist/style.css": "./dist/style.css"
  }
}

{
  "name": "@navikt/ft-sak-infosider",
  "description": "Diverse info- og feil-sider",
  "version": "2.4.8",
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7020"
  },
  "dependencies": {
    "@navikt/ds-css": "7.4.2",
    "@navikt/ds-react": "7.4.2",
    "@navikt/ft-utils": "^2.4.8",
    "react": "18.3.1",
    "react-intl": "6.8.4"
  },
  "peerDependencies": {
    "@navikt/ds-css": "7.x",
    "@navikt/ds-react": "7.x",
    "@navikt/ft-utils": "2.x",
    "react": "18.3.1",
    "react-intl": "6.x"
  },
  "devDependencies": {
    "@storybook/addon-actions": "8.4.1",
    "@storybook/react": "8.4.1",
    "@storybook/react-vite": "8.4.1",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "14.5.2",
    "cross-env": "7.0.3",
    "eslint": "9.13.0",
    "storybook": "8.4.1",
    "stylelint": "16.10.0",
    "typescript": "5.6.3",
    "vite": "5.4.10",
    "vitest": "2.1.4"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

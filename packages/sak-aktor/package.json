{
  "name": "@navikt/ft-sak-aktor",
  "description": "Komponent som viser en liste med saker knyttet til en akt√∏r-id",
  "version": "1.3.4",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --colors",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\"",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\"",
    "less:lint": "stylelint \"src/**/*.less\"",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly --downlevelIteration --resolveJsonModule --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules",
    "storybook": "cross-env start-storybook -p 7013"
  },
  "dependencies": {
    "@navikt/ds-css": "2.0.8",
    "@navikt/ds-react": "2.0.8",
    "@navikt/ft-kodeverk": "^1.0.24",
    "@navikt/ft-plattform-komponenter": "^1.4.2",
    "@navikt/ft-sak-visittkort": "^1.3.4",
    "@navikt/ft-types": "^1.0.36",
    "@navikt/ft-ui-komponenter": "^1.3.4",
    "@navikt/ft-utils": "^1.1.11",
    "react": "18.2.0",
    "react-intl": "6.2.5"
  },
  "peerDependencies": {
    "@navikt/ds-css": "2.0.8",
    "@navikt/ds-react": "2.0.8",
    "@navikt/ft-kodeverk": "^1.x",
    "@navikt/ft-plattform-komponenter": "^1.x",
    "@navikt/ft-sak-visittkort": "1.x",
    "@navikt/ft-types": "^1.x",
    "@navikt/ft-ui-komponenter": "1.x",
    "@navikt/ft-utils": "1.x",
    "react": "18.2.0",
    "react-intl": "6.2.5"
  },
  "devDependencies": {
    "@navikt/ft-storybook-utils": "^1.0.7",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/react": "13.4.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": "./dist/index.js",
    "./dist/style.css": "./dist/style.css"
  }
}

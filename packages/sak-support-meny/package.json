{
  "name": "@navikt/ft-sak-support-meny",
  "description": "Sidemeny som lar saksbehandling velge mellom 'Send melding', 'Dokumenter', 'Historikk', og beslutterinformasjon",
  "version": "2.0.11",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules",
    "build-storybook": "storybook build -o ../../.storybook-static-build/@navikt/ft-sak-support-meny",
    "storybook": "storybook dev --quiet -p 7016"
  },
  "dependencies": {
    "@navikt/ds-css": "2.8.4",
    "@navikt/ft-kodeverk": "^2.0.9",
    "@navikt/ft-types": "^2.0.9",
    "@navikt/ft-ui-komponenter": "^2.0.11",
    "@navikt/ft-utils": "^2.0.10",
    "classnames": "2.3.2",
    "react": "18.2.0",
    "react-intl": "6.3.2"
  },
  "peerDependencies": {
    "@navikt/ds-css": "2.6.0",
    "@navikt/ft-kodeverk": "1.x",
    "@navikt/ft-types": "1.x",
    "@navikt/ft-ui-komponenter": "1.x",
    "@navikt/ft-utils": "1.x",
    "classnames": "2.3.2",
    "react": "18.2.0",
    "react-intl": "6.2.10"
  },
  "devDependencies": {
    "@storybook/addon-actions": "7.0.0-rc.5",
    "@storybook/react": "7.0.0-rc.5",
    "@storybook/react-vite": "7.0.0-rc.5",
    "@storybook/testing-react": "2.0.0-next.0",
    "@testing-library/react": "14.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.36.0",
    "storybook": "7.0.0-rc.5",
    "stylelint": "15.3.0",
    "typescript": "5.0.2",
    "vite": "4.2.1",
    "vitest": "0.29.7"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

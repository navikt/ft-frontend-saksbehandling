{
  "name": "@navikt/ft-sak-support-meny",
  "description": "Sidemeny som lar saksbehandling velge mellom 'Send melding', 'Dokumenter', 'Historikk', og beslutterinformasjon",
  "version": "2.0.0",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules",
    "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook --quiet -p 7016"
  },
  "dependencies": {
    "@navikt/ds-css": "2.1.7",
    "@navikt/ft-kodeverk": "2.0.0",
    "@navikt/ft-types": "2.0.0",
    "@navikt/ft-ui-komponenter": "2.0.0",
    "@navikt/ft-utils": "2.0.0",
    "classnames": "2.3.2",
    "react": "18.2.0",
    "react-intl": "6.2.8"
  },
  "peerDependencies": {
    "@navikt/ds-css": "2.1.7",
    "@navikt/ft-kodeverk": "1.x",
    "@navikt/ft-types": "1.x",
    "@navikt/ft-ui-komponenter": "1.x",
    "@navikt/ft-utils": "1.x",
    "classnames": "2.3.2",
    "react": "18.2.0",
    "react-intl": "6.2.8"
  },
  "devDependencies": {
    "@storybook/react": "6.5.16",
    "@storybook/testing-react": "1.3.0",
    "@svgr/webpack": "6.5.1",
    "@testing-library/react": "13.4.0",
    "cross-env": "7.0.3",
    "eslint": "8.35.0",
    "stylelint": "15.2.0",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vitest": "0.29.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

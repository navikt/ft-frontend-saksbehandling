{
  "name": "@navikt/ft-sak-support-meny",
  "description": "Sidemeny som lar saksbehandling velge mellom 'Send melding', 'Dokumenter', 'Historikk', og beslutterinformasjon",
  "version": "1.2.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --colors",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\"",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\"",
    "less:lint": "stylelint \"src/**/*.less\"",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly --downlevelIteration --resolveJsonModule --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules",
    "storybook": "cross-env start-storybook -p 7016"
  },
  "dependencies": {
    "@navikt/ft-kodeverk": "^1.0.21",
    "@navikt/ft-types": "^1.0.32",
    "@navikt/ft-ui-komponenter": "^1.2.0",
    "@navikt/ft-utils": "^1.1.7",
    "classnames": "2.3.2",
    "react": "18.2.0",
    "react-intl": "6.2.1"
  },
  "peerDependencies": {
    "@navikt/ft-kodeverk": "^1.x",
    "@navikt/ft-types": "^1.x",
    "@navikt/ft-ui-komponenter": "1.x",
    "@navikt/ft-utils": "1.x",
    "classnames": "2.3.2",
    "react": "18.2.0",
    "react-intl": "6.2.1"
  },
  "devDependencies": {
    "@storybook/testing-react": "1.3.0",
    "@svgr/webpack": "6.5.1",
    "@testing-library/react": "13.4.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": "./dist/index.js",
    "./dist/style.css": "./dist/style.css"
  }
}

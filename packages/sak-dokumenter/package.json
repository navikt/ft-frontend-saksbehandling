{
  "name": "@navikt/ft-sak-dokumenter",
  "description": "Viser en dokumentoversikt. Saksbehandler kan velge å åpne en eller flere dokumenter.",
  "version": "2.0.36",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7017"
  },
  "dependencies": {
    "@navikt/aksel-icons": "4.1.7",
    "@navikt/ds-css": "4.1.7",
    "@navikt/ds-react": "4.1.7",
    "@navikt/ft-kodeverk": "^2.1.13",
    "@navikt/ft-types": "^2.1.15",
    "@navikt/ft-ui-komponenter": "^2.0.36",
    "@navikt/ft-utils": "^2.0.32",
    "react": "18.2.0",
    "react-intl": "6.4.4"
  },
  "peerDependencies": {
    "@navikt/aksel-icons": "3.1.2",
    "@navikt/ds-css": "2.8.11",
    "@navikt/ds-react": "2.8.11",
    "@navikt/ft-kodeverk": "2.x",
    "@navikt/ft-types": "2.x",
    "@navikt/ft-ui-komponenter": "2.x",
    "@navikt/ft-utils": "2.x",
    "react": "18.2.0",
    "react-intl": "6.3.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "7.0.22",
    "@storybook/react": "7.0.23",
    "@storybook/react-vite": "7.0.22",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "cross-env": "7.0.3",
    "eslint": "8.43.0",
    "storybook": "7.0.22",
    "stylelint": "15.8.0",
    "typescript": "5.0.4",
    "vite": "4.3.9",
    "vitest": "0.32.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

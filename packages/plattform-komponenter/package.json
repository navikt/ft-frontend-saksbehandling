{
  "name": "@navikt/ft-plattform-komponenter",
  "description": "Pakke som tilbyr sammensatte komponenter som f.eks. menyer",
  "version": "5.1.0",
  "license": "MIT",
  "type": "module",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7007"
  },
  "dependencies": {
    "@navikt/aksel-icons": "7.5.1",
    "@navikt/ds-css": "7.5.1",
    "@navikt/ds-react": "7.5.1",
    "@navikt/ft-utils": "workspace:^",
    "classnames": "2.5.1"
  },
  "peerDependencies": {
    "@navikt/aksel-icons": "7.x",
    "@navikt/ds-css": "7.x",
    "@navikt/ds-react": "7.x",
    "@popperjs/core": "2.11.8",
    "classnames": "2.5.1",
    "lodash.throttle": "4.1.1",
    "prop-types": "15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-popper": "2.3.0"
  },
  "peerDependenciesMeta": {
    "react-popper": {
      "optional": true
    }
  },
  "devDependencies": {
    "@popperjs/core": "2.11.8",
    "@storybook/addon-actions": "8.4.2",
    "@storybook/react": "8.4.2",
    "@storybook/react-vite": "8.4.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "14.5.2",
    "classnames": "2.5.1",
    "cross-env": "7.0.3",
    "eslint": "9.14.0",
    "lodash.throttle": "4.1.1",
    "prop-types": "15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-popper": "2.3.0",
    "storybook": "8.4.2",
    "stylelint": "16.10.0",
    "typescript": "5.6.3",
    "vite": "5.4.10",
    "vitest": "2.1.4"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

{
  "name": "@navikt/ft-plattform-komponenter",
  "description": "Pakke som tilbyr sammensatte komponenter som f.eks. menyer",
  "version": "2.0.12",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules",
    "build-storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider build-storybook -o .storybook-static-build",
    "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook --quiet -p 7007"
  },
  "dependencies": {
    "@navikt/ds-css": "2.8.4",
    "@navikt/ds-icons": "2.8.4",
    "@navikt/ds-react": "2.8.4",
    "@navikt/ft-utils": "^2.0.10",
    "classnames": "2.3.2"
  },
  "peerDependencies": {
    "@popperjs/core": "2.11.6",
    "classnames": "2.3.2",
    "dayjs": "1.11.7",
    "lodash.throttle": "4.1.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-collapse": "5.1.1",
    "react-dom": "18.2.0",
    "react-popper": "2.3.0"
  },
  "peerDependenciesMeta": {
    "react-popper": {
      "optional": true
    }
  },
  "devDependencies": {
    "@popperjs/core": "2.11.7",
    "@storybook/react": "7.0.2",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "classnames": "2.3.2",
    "cross-env": "7.0.3",
    "dayjs": "1.11.7",
    "eslint": "8.36.0",
    "lodash.throttle": "4.1.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-collapse": "5.1.1",
    "react-dom": "18.2.0",
    "react-popper": "2.3.0",
    "stylelint": "15.3.0",
    "typescript": "5.0.2",
    "vite": "4.2.1",
    "vitest": "0.29.7"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}

{
  "name": "@navikt/ft-plattform-komponenter",
  "description": "Pakke som tilbyr sammensatte komponenter som f.eks. menyer",
  "version": "1.4.6",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --colors",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\"",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\"",
    "less:lint": "stylelint \"src/**/*.less\"",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly --downlevelIteration --resolveJsonModule --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules",
    "storybook": "cross-env start-storybook -p 7007"
  },
  "dependencies": {
    "@navikt/ds-css": "2.0.12",
    "@navikt/ds-icons": "2.0.12",
    "@navikt/ds-react": "2.0.12",
    "@navikt/ft-utils": "^1.1.13"
  },
  "peerDependencies": {
    "@popperjs/core": "2.11.6",
    "classnames": "2.3.2",
    "dayjs": "1.11.7",
    "lodash.throttle": "4.1.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-collapse": "^5.1.1",
    "react-dom": "^17.0.2",
    "react-popper": "^2.2.5"
  },
  "peerDependenciesMeta": {
    "react-popper": {
      "optional": true
    }
  },
  "devDependencies": {
    "@popperjs/core": "2.11.6",
    "@storybook/react": "6.5.16",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "lodash.throttle": "^4.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-collapse": "^5.1.1",
    "react-dom": "^18.2.0",
    "react-popper": "^2.2.5"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": "./dist/index.js",
    "./dist/style.css": "./dist/style.css"
  }
}
